    "strokeColor": [0, 0, 255, 1],
    "strokeWidth": 1,
    "strokeDash": [4],
    "fillColor": [255, 0, 255, 0.33],
    "circleRadius": 10,
    "textFill": "black",
    "textStroke": "white",
    "textFont": "11pt sans-serif"
